else if (!string.IsNullOrEmpty(script) && !string.IsNullOrEmpty(function_name))
	result = instance2.ExecutePythonScript<PyTuple>(script, function_name, param);
else result = null;
});
t.Wait(TimeSpan.FromSeconds(GlobalWait));

if(result == null){
